{
    "manifest_version": 2,
    "name": "百度云盘转存加强",
    "version": "0.1",
    "description": "百度云盘转存加强,每次只能转存100个文件/夹的限制",
    "browser_action": {
        "default_icon": "logo.png"
    },
    "permissions": [
        "http://pan.baidu.com/*"
    ],
   "content_scripts": [ {
      "js": ["script.js" ],
      "matches": [ "http://pan.baidu.com/*" ],
      "run_at": "document_end"
   }]
}